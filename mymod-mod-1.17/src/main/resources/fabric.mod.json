{
  "schemaVersion": 1,
  "id": "mymod",
  "version": "${version}",

  "name": "My mod",
  "description": "My Fabric Mod.",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/mymod/icon.png",

  "environment": "*",
